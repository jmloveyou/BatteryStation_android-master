<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- outer circle -->
    <item>
        <shape android:shape="oval" >
            <solid android:color="@color/transparent" />
            <stroke
                android:width="0.5dp"
                android:color="@color/pink_01"/>
            <size android:width="64dp"
                android:height="64dp" />
        </shape>
    </item>

    <!-- inner circle -->
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval" >
            <solid android:color="@color/pink_01" />
        </shape>
    </item>

</layer-list>